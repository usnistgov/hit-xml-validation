	<xsd:complexType name="PAReasonCodeType">
		<xsd:sequence>
			<xsd:element name="PACaseID" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonCode" type="ecl:ReasonCode10" maxOccurs="10"/>
			<xsd:element name="AuthorizationNumber" type="datatypes:an1..140" minOccurs="0"/>
			<xsd:element name="AuthorizationDetail" type="structures:PAAuthorizationDetails" minOccurs="0" maxOccurs="5"/>
			<xsd:element name="AuthorizationPeriod" type="structures:DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="PANote" type="datatypes:an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PAResponseChoiceType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PAResponseChoiceType">
		<xsd:choice>
			<xsd:element name="Approved" type="structures:PAApprovedDetail">
				<xsd:annotation>
					<xsd:documentation>Either AuthorizationDetail or AuthorizationPeriod must be sent.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Denied" type="structures:PADeniedType"/>
			<xsd:element name="Open" type="structures:PAOpenTypeResponse"/>
			<xsd:element name="Closed" type="structures:PAReasonClosedCodeType"/>
			<xsd:element name="PartiallyDenied" type="structures:PAPartiallyDeniedDetail">
				<xsd:annotation>
					<xsd:documentation>Either AuthorizationDetail or AuthorizationPeriod must be sent.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
