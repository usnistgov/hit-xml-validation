	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AllergyRestrictedChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AllergyRestrictedChoice">
		<xsd:complexContent>
			<xsd:restriction base="structures:AllergyChoice">
				<xsd:choice>
					<xsd:element name="NoKnownAllergies" type="ecl:YesCode">
						<xsd:annotation>
							<xsd:documentation>If the AllergyOrAdverseEvent element is sent, it must include all known patient allergies, or it must designate NoKnownAllergies = Y.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Allergies" type="structures:AllergyRestricted" maxOccurs="unbounded"/>
				</xsd:choice>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AlternateContactStructure  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AlternateContactStructure">
		<xsd:annotation>
			<xsd:documentation>Alternate contact relationship to the patient.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="datatypes:NameType">
				<xsd:annotation>
					<xsd:documentation>See PatientFullAbstract for Definition and Usage.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FormerName" type="datatypes:NameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="datatypes:AddressType">
				<xsd:annotation>
					<xsd:documentation>See PatientFullAbstract for Definition and Usage.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
				<xsd:annotation>
					<xsd:documentation>See PatientFullAbstract for Definition and Usage.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AlternateContactRelationship" type="ecl:EligibilityPatientRelationshipCode">
				<xsd:annotation>
					<xsd:documentation>See field 247 – Eligibility/Patient Relationship Code values.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AnswerChoiceCommonType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AnswerChoiceCommonType">
		<xsd:sequence>
			<xsd:element name="ChoiceID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the answer choice for a question. Must be unique per answer choice for the question.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SequenceNumber" type="datatypes:n1..3">
				<xsd:annotation>
					<xsd:documentation>Sequence number assigned by the payer to indicate the order the answer choices for a question should be displayed to the provider. Must be unique per answer choice for the question.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ChoiceText" type="datatypes:an1..2000">
				<xsd:annotation>
					<xsd:documentation>Answer choice text. Vendor to display to the prescriber as the answer choice text for Select question type.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalFreeTextIndicator" type="ecl:AdditionalFreeTextIndicatorTypeCode">
				<xsd:annotation>
					<xsd:documentation>Indicates if the prescriber is allowed or is required to supply additional free text with their answer to the question.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReference" type="structures:CodedReference" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="NextQuestionID" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is dependent on the answer to the question (i.e., answer choice or numeric condition). Required when the DefaultNextQuestionID for the question is not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AnswerChoiceForNumericType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AnswerChoiceForNumericType">
		<xsd:sequence>
			<xsd:element name="NextQuestionCondition" type="structures:NumericType"/>
			<xsd:element name="Answer" type="datatypes:an1..2000"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AnswerChoiceForPARequest  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AnswerChoiceForPARequest">
		<xsd:sequence>
			<xsd:element name="ChoiceID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the answer choice for a question. Must be unique per answer choice for the question.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalFreeText" type="datatypes:an1..2000" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AnswerForTextType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AnswerForTextType">
		<xsd:complexContent>
			<xsd:extension base="structures:FreeTextTypeBasic">
				<xsd:sequence>
					<xsd:element name="Answer" type="structures:CompletedAnswerChoiceForFreeTextType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Attachment  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Attachment">
		<xsd:sequence>
			<xsd:element name="AttachmentSource" type="datatypes:an">
				<xsd:annotation>
					<xsd:documentation>The Source of the Info</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AttachmentData" type="xsd:base64Binary">
				<xsd:annotation>
					<xsd:documentation>all attached information is Base64 encoded</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="structures:ClinicalInformation"/>
			<xsd:element name="MIMEType" type="ecl:MIMETypeCode">
				<xsd:annotation>
					<xsd:documentation>Defines the content nature of the attachmentData.  Internet standard defined in RFC 2045, RFC 2046, RFC 2047, RFC 4288, RFC 4289 and RFC 2049. See ECL for currently recognized and enumerated MIME Types for purposes in NCPDP.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AttachmentWithControlNumber  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AttachmentWithControlNumber">
		<xsd:annotation>
			<xsd:documentation>For electronic prior authorizations transactions, Attachments are supported both to answer or clarify a specific question, or more than one question. If an attachment is sent to address a specific question, then the CodedReferenceAnswer Attachment element is used.  If an attachment will address multiple questions, then SeeTransactionLevelAttachmentControlNumber indicates an attachment at the transaction level should be viewed.  For example, if one clinical document can be used to answer multiple questions, the document can be sent once and referred to for the multiple answers.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AttachmentControlNumber" type="datatypes:an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the control number of the attachment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AttachmentSource" type="datatypes:an">
				<xsd:annotation>
					<xsd:documentation>The Source of the Info</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AttachmentData" type="xsd:base64Binary">
				<xsd:annotation>
					<xsd:documentation>All attached information is Base64 encoded</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="structures:ClinicalInformation" minOccurs="0"/>
			<xsd:element name="MIMEType" type="ecl:MIMETypeCode">
				<xsd:annotation>
					<xsd:documentation>Defines the content nature of the attachmentData.  Internet standard defined in RFC 2045, RFC 2046, RFC 2047, RFC 4288, RFC 4289 and RFC 2049. See ECL for currently recognized and enumerated MIME Types for purposes in NCPDP.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Attachments  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Attachments">
		<xsd:sequence>
			<xsd:element name="Attachment" type="structures:Attachment" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  BaseBenefitsCoordination  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="BaseBenefitsCoordination">
		<xsd:complexContent>
			<xsd:restriction base="structures:BaseBenefitsCoordinationAbstract">
				<xsd:sequence>
					<xsd:element name="PayerIdentification" type="datatypes:PayerID" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>For Census - for Private Pay, this will not be sent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PayerName" type="datatypes:an1..70" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Census - if Private Pay – This is the responsible Party Name.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CardholderID" type="datatypes:an1..35" minOccurs="0"/>
					<xsd:element name="CardHolderName" type="datatypes:NameType" minOccurs="0"/>
					<xsd:element name="GroupID" type="datatypes:an1..35" minOccurs="0"/>
					<xsd:element name="PayerResponsibilityCode" type="ecl:PayerResponsibilityCode" minOccurs="0"/>
					<xsd:element name="PatientRelationship" type="ecl:PatientRelationshipCode" minOccurs="0"/>
					<xsd:element name="PersonCode" type="datatypes:n1..3" minOccurs="0"/>
					<xsd:element name="GroupName" type="datatypes:an1..70" minOccurs="0"/>
					<xsd:element name="Address" type="datatypes:AddressType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>For Census - Responsible Party’s address</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>For RxChangeRequest: The payer-based telephone number exchanged between the pharmacy and the prescriber is the prior authorization telephone number at the payer that the pharmacy may have knowledge of (rather than the payer’s phone number for member questions or billing questions etc at the payer).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Consent" type="ecl:ConsentCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Note: If Patient Consent = N, it is expected the RxHistory transactions would not be sent.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PBMMemberID" type="datatypes:an1..80" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Payer assigned Unique Member ID.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ResponsibleParty" type="datatypes:NameType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>For Census, Full Name of Responsible Party.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ServiceTypeCoded" type="ecl:ServiceTypeCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Medication list contains all current medication orders as of the current date and time of the response, for the patient indicated. Current status is determined by the point of care responder. “Current” is medication orders which have not been discontinued.

Value “C” is “Not used” to request medication history from pharmacy or payer/processor. 

RxHistoryRequest:  When this field contains a value of “C” in the Medication History request message, Date is not sent. 

RxHistoryResponse: The response echoes back the “C” value in this field.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
