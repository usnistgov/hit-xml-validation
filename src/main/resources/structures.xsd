				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Denied" type="structures:PADeniedTypeForAppeal"/>
			<xsd:element name="Open" type="structures:PAOpenTypeResponseForAppeal"/>
			<xsd:element name="Closed" type="structures:PAReasonClosedCodeTypeForAppeal"/>
			<xsd:element name="PartiallyDenied" type="structures:PAApprovedDetailForAppeal">
				<xsd:annotation>
					<xsd:documentation>Either AuthorizationDetail or AuthorizationPeriod must be sent.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PAResponseChoiceTypeRestricted  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PAResponseChoiceTypeRestricted">
