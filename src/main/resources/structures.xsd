			<xsd:restriction base="structures:SupervisorTypeAbstract">
				<xsd:choice>
					<xsd:element name="NonVeterinarian" type="structures:SupervisorMandatoryAddress"/>
					<xsd:element name="Veterinarian" type="structures:VetSupervisorMirrorSupervisorMandatoryAddress"/>
