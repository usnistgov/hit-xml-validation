			</xsd:element>
			<xsd:element name="Specialty" type="datatypes:an1..10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value A.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacist" type="structures:Pharmacist" minOccurs="0"/>
