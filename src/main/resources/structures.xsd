					<xsd:element name="Veterinarian" type="structures:VetSupervisorMirrorSupervisorOptional"/>
				</xsd:choice>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  SupervisorType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="SupervisorType">
		<xsd:complexContent>
			<xsd:restriction base="structures:SupervisorTypeAbstract">
				<xsd:choice>
					<xsd:element name="NonVeterinarian" type="structures:Supervisor"/>
					<xsd:element name="Veterinarian" type="structures:VetSupervisorMirrorSupervisor"/>
				</xsd:choice>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  SupervisorTypeAbstract  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="SupervisorTypeAbstract">
