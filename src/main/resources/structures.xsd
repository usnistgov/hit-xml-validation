					<xsd:element name="LotNumber" type="datatypes:LotNumber" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="RxFillIndicator" type="ecl:RxFillIndicatorType" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="Patient" type="structures:Patient" minOccurs="0"/>
					<xsd:element name="Wound" type="structures:WoundStructure" minOccurs="0" maxOccurs="9"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  HistoryPrescriber  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="HistoryPrescriber">
		<xsd:sequence>
			<xsd:element name="Identification" type="datatypes:NonVeterinarianID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>One occurrence is the prescriber number. One occurrence may be used to identify the clinic. At least one occurrence must contain the Prescriber individual (not organizational) NPI Number. DEANumber is sent when applicable.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Specialty" type="datatypes:an1..10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value A for the NUCC taxonomy list.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PracticeLocation" type="structures:PracticeLocationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="datatypes:NameType" minOccurs="0"/>
			<xsd:element name="FormerName" type="datatypes:NameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="datatypes:AddressType" minOccurs="0"/>
			<xsd:element name="PrescriberAgent" type="structures:NameComposite" minOccurs="0"/>
			<xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  HistoryPrescriberChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="HistoryPrescriberChoice">
		<xsd:choice>
			<xsd:element name="NonVeterinarian" type="structures:HistoryPrescriber"/>
			<xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorHistoryPrescriber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  IVAdministrationType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="IVAdministrationType">
		<xsd:sequence>
			<xsd:element name="NumberOfLumens" type="datatypes:an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the number of lumens in the IV access line that will be used for administering the medication.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DiluentAmount" type="datatypes:QuantityWithoutSecondQuantity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the amount of diluent (which will have a separate order) to be used for administration. Concepts of the intended or actual diluent amount unit of measure.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationGauge" type="datatypes:n" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the size (gauge) of the needle or catheter being used for medication administration, i.e. 14, 18, 20.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationBrand" type="datatypes:an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the specific Brand of the PICC line used for central IV access.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationLength" type="datatypes:an" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the size (length) of the PICC line used for central IV access.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SpecificAdministrationPump" type="ecl:BooleanCode" minOccurs="0"/>
			<xsd:element name="IVAccessType" type="datatypes:SNOMEDCode"/>
			<xsd:element name="IVAccessDeviceType" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IVAccessDeviceTypeDescription" type="datatypes:an" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates the specific device used for central IV access, if applicable i.e. implanted port, PICC line, tunneled line, non-tunneled line.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IVAccessDeviceType" type="datatypes:SNOMEDCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The code representing the IVAccessDeviceType. It is the intended specific device used for central IV access.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="IVAccessCatheterTip" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IVAccessCatheterTipDescription" type="datatypes:an" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates the specific structure of the catheter tip for the specific device used for central or midline IV access., if applicable i.e. valved, non-valved.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IVAccessCatheterTipType" type="datatypes:SNOMEDCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The code representing the IVAccessCatheterTipType. It is the intended specific structure of the catheter tip for the specific device used for central or midline IV access.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="IVInfusion" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IVInfusionDescription" type="datatypes:an" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indicates the intended type of IV infusion i.e. continuous, intermittent, TOTAL perenteral nutrition (TPN), PARTIAL perenteral nutrition (PPN), maintenance.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IVInfusionType" type="datatypes:SNOMEDCode" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The code representing the IVInfusionTypeType. It is the intended type of IV infusion.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Indication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Indication">
		<xsd:sequence>
			<xsd:element name="IndicationPrecursor" type="datatypes:SigCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as As needed, as directed, when, for…</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Indication" type="datatypes:SigCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as Blood Pressure, Finger Stick Blood Glucose</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:sequence minOccurs="0">
				<xsd:element name="IndicationValue" type="datatypes:SigCode">
					<xsd:annotation>
						<xsd:documentation>Examples such as equal than, greater than, less than</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="IndicationVariableModifier" type="datatypes:ANDORTO" minOccurs="0"/>
				<xsd:element name="IndicationValue" type="datatypes:SigCode" minOccurs="0"/>
			</xsd:sequence>
			<xsd:element name="IndicationValueUnit" type="datatypes:SigCode" minOccurs="0"/>
			<xsd:element name="IndicationValueUnitOfMeasure" type="datatypes:SigCode" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Instruction  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Instruction">
		<xsd:choice>
			<xsd:sequence>
				<xsd:element name="InstructionIndicator" type="ecl:InstructionIndicatorCodes">
					<xsd:annotation>
						<xsd:documentation>When structured Sig is as directed based on medical encounter.
Skip Dose. (Timing is situational.)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence minOccurs="0" maxOccurs="49">
					<xsd:element name="MultipleTimingModifier" type="datatypes:ANDOR" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>May be used to linked to any of the timing methods -– Administration Timing and/or Frequency and/or Interval.  For example,” take each morning, before bed and every four hours as needed.”</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="TimingAndDuration" type="structures:TimingAndDuration"/>
				</xsd:sequence>
			</xsd:sequence>
			<xsd:sequence>
				<xsd:choice>
					<xsd:element name="AdministrationIndicator" type="ecl:AdministrationIndicatorCodes">
						<xsd:annotation>
							<xsd:documentation>When structured Sig is as directed per manufacturer's packaging.
Skip Dose.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:sequence>
						<xsd:element name="DoseAdministration" type="structures:DoseAdministration"/>
						<xsd:element name="TimingAndDuration" type="structures:TimingAndDuration">
							<xsd:annotation>
								<xsd:documentation>This element is used to provide instruction about the timing of the Sig -– when/how often/at what rate/for how long -– the medication is to be taken. Specific time fields must be Consistent with ISO format.</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:sequence minOccurs="0" maxOccurs="49">
							<xsd:element name="MultipleTimingModifier" type="datatypes:ANDOR" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>May be used to link any of the timing methods – Administration Timing and/or Frequency and/or Interval.  For example,take each morning, before bed and every four hours as needed.</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="TimingAndDuration" type="structures:TimingAndDuration"/>
						</xsd:sequence>
					</xsd:sequence>
				</xsd:choice>
				<xsd:sequence minOccurs="0">
					<xsd:element name="IndicationForUse" type="structures:Indication" maxOccurs="50">
						<xsd:annotation>
							<xsd:documentation>Defines the indication for use of the medication as meant to be conveyed to the patient. Note:  The indication for use of the medication as meant to be conveyed to the patient may differ from the clinical indication for use.  As an example, take for sleep may be conveyed to the patient; the clinical indication may be insomnia.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="IndicationClarifyingFreeText" type="datatypes:an1..255" minOccurs="0"/>
				</xsd:sequence>
				<xsd:element name="MaximumDoseRestriction" type="structures:MaximumDoseRestriction" minOccurs="0" maxOccurs="50">
					<xsd:annotation>
						<xsd:documentation>Defines a maximum or dose limit, as specified by the prescriber. The value entered may or may not match with the information provided by clinical decision support tools.  Professional judgment should be used when discrepancies occur.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  IntervalTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="IntervalTiming">
		<xsd:sequence>
			<xsd:element name="IntervalNumericValue" type="datatypes:n"/>
			<xsd:sequence minOccurs="0">
				<xsd:element name="VariableIntervalModifier" type="datatypes:ANDORTO"/>
				<xsd:element name="IntervalNumericValue" type="datatypes:n"/>
			</xsd:sequence>
			<xsd:element name="IntervalUnits" type="datatypes:SigCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryAddressPharmacy  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryAddressPharmacy">
		<xsd:complexContent>
			<xsd:restriction base="structures:MandatoryPharmacy">
				<xsd:sequence>
					<xsd:element name="Identification" type="datatypes:PharmacyID">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="datatypes:an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacist" type="structures:Pharmacist" minOccurs="0"/>
					<xsd:element name="BusinessName" type="datatypes:an1..70">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="datatypes:MandatoryAddressType">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryPharmacy  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryPharmacy">
		<xsd:annotation>
			<xsd:documentation>Value P2</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="structures:Pharmacy">
				<xsd:sequence>
					<xsd:element name="Identification" type="datatypes:PharmacyID">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="datatypes:an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacist" type="structures:Pharmacist" minOccurs="0"/>
					<xsd:element name="BusinessName" type="datatypes:an1..70">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="datatypes:AddressType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
						<xsd:annotation>
							<xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryPrescriber  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryPrescriber">
		<xsd:annotation>
			<xsd:documentation>Value PC</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="structures:Prescriber">
				<xsd:sequence>
					<xsd:element name="Identification" type="datatypes:NonVeterinarianID">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="datatypes:an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PracticeLocation" type="structures:PracticeLocationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Name" type="datatypes:NameType">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FormerName" type="datatypes:NameType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="datatypes:MandatoryAddressType">
						<xsd:annotation>
							<xsd:documentation>The prescriber address on a NEWRX must be the practice address.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrescriberAgent" type="structures:NameComposite" minOccurs="0"/>
					<xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MandatoryPrescriberChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MandatoryPrescriberChoice">
		<xsd:complexContent>
			<xsd:restriction base="structures:HistoryPrescriberChoice">
				<xsd:choice>
					<xsd:element name="NonVeterinarian" type="structures:MandatoryPrescriber"/>
					<xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorMandatoryPrescriber"/>
				</xsd:choice>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MaximumDoseRestriction  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MaximumDoseRestriction">
		<xsd:sequence>
			<xsd:element name="MaximumDoseRestrictionNumericValue" type="datatypes:n"/>
			<xsd:choice>
				<xsd:element name="MaximumDoseRestrictionForm" type="datatypes:DoseUnitOfMeasureCode"/>
				<xsd:element name="MaximumDoseRestrictionUnits" type="datatypes:SigCode"/>
			</xsd:choice>
			<xsd:element name="MaximumDoseRestrictionDurationValue" type="datatypes:n"/>
			<xsd:element name="MaximumDoseRestrictionDurationUnit" type="datatypes:SigCode"/>
			<xsd:element name="MaximumDoseRestrictionClarifyingFreeText" type="datatypes:an1..255" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementDuration  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementDuration">
		<xsd:sequence>
			<xsd:element name="MeasurementDurationNumericValue" type="datatypes:n"/>
			<xsd:element name="MeasurementDurationText" type="datatypes:SigCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementDurationTriggerTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementDurationTriggerTiming">
		<xsd:sequence>
			<xsd:element name="MeasurementTrigger" type="datatypes:SigCode"/>
			<xsd:element name="MeasurementDurationClarifyingFreeText" type="datatypes:an1..255" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementFrequencyTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementFrequencyTiming">
		<xsd:sequence>
			<xsd:element name="MeasurementFrequencyNumericValue" type="datatypes:n"/>
			<xsd:sequence minOccurs="0">
				<xsd:element name="MeasurementVariableFrequencyModifier" type="datatypes:ANDORTO">
					<xsd:annotation>
						<xsd:documentation>Used to express when there is more than one Measurement Frequency as to whether the frequencies are all required to be used (AND) or if any of the frequencies can be used (OR/TO).</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="MeasurementFrequencyNumericValue" type="datatypes:n"/>
			</xsd:sequence>
			<xsd:element name="MeasurementrequencyUnits" type="datatypes:SigCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementIntervalTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementIntervalTiming">
		<xsd:sequence>
			<xsd:element name="MeasurementIntervalNumericValue" type="datatypes:n"/>
			<xsd:sequence minOccurs="0">
				<xsd:element name="MeasurementVariableIntervalModifier" type="datatypes:ANDORTO">
					<xsd:annotation>
						<xsd:documentation>Used to express when there is more than one Interval as to whether the intervals are all required to be used (AND) or if any of the intervals can be used (OR/TO).</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="MeasurementIntervalNumericValue" type="datatypes:n"/>
			</xsd:sequence>
			<xsd:element name="MeasurementIntervalUnits" type="datatypes:SigCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MeasurementTiming  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MeasurementTiming">
		<xsd:sequence>
			<xsd:sequence minOccurs="0">
				<xsd:element name="MeasurementTimingNumericValue" type="datatypes:n"/>
				<xsd:element name="VariableMeasurementTimingModifier" type="datatypes:ANDORTO">
					<xsd:annotation>
						<xsd:documentation>Used to express when there is more than one time as to whether the times are all required to be used (AND) or if any of the times can be used (OR). (TO) is used if the Measurement Timing provides for a range.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:sequence minOccurs="0">
					<xsd:element name="AdministrationTimingNumericValue" type="datatypes:n"/>
				</xsd:sequence>
				<xsd:element name="MeasurementTimingUnits" type="datatypes:SigCode"/>
			</xsd:sequence>
			<xsd:element name="MeasurementTimingModifier" type="datatypes:SigCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Examples such as before, during, with, without, after, while, at., on, start, end.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MeasurementTimingEvent" type="datatypes:SigCode"/>
			<xsd:element name="MeasurementTimingClarifyingFreeText" type="datatypes:an1..255" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to add clarity to the titration measurement timing for elements that cannot be codified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Medication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Medication">
		<xsd:annotation>
			<xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="structures:MedicationAbstract">
				<xsd:sequence>
					<xsd:element name="DrugDescription" type="datatypes:an1..105">
						<xsd:annotation>
							<xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugCoded" type="datatypes:DrugCoded" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="Quantity" type="datatypes:QuantityAbstract" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
						</xsd:annotation>
