				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherCompoundInformation" type="structures:OtherCompoundInformationType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Sig" type="structures:Sig" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
				</xsd:annotation>
