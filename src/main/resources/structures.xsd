			<xsd:element name="Service" type="structures:ServiceStructure" minOccurs="0"/>
			<xsd:element name="IVAdministration" type="structures:IVAdministrationType" minOccurs="0"/>
			<xsd:element name="SelfAdministrationAllowed" type="ecl:BooleanCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AnticipatedDischargeDate" type="datatypes:DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrentTreatmentCycle" type="datatypes:n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfCyclesPlanned" type="datatypes:n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateRecertified" type="datatypes:DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The date when the prescriber recertifies the medication order.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Wound" type="structures:WoundStructure" minOccurs="0" maxOccurs="9"/>
			<xsd:element name="PrescriberCheckedREMS" type="ecl:PrescriberCheckedREMSCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies if the prescribing system has performed an inquiry to the REMS Administrator in order to verify the REMS component of the prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSPatientRiskCategory" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identifies the risk of adverse event based on variables of the patient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="datatypes:an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Titration" type="structures:TitrationType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  NameComposite  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="NameComposite">
		<xsd:sequence>
			<xsd:element name="Name" type="datatypes:NameType"/>
			<xsd:element name="FormerName" type="datatypes:NameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  NewRxPrescribedMedication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="NewRxPrescribedMedication">
		<xsd:complexContent>
			<xsd:restriction base="structures:MedicationAbstract">
				<xsd:sequence>
					<xsd:element name="DrugDescription" type="datatypes:an1..105">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugCoded" type="datatypes:DrugCoded" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Quantity" type="datatypes:QuantityForPrescribed"/>
					<xsd:element name="DaysSupply" type="datatypes:n1..3" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="WrittenDate" type="datatypes:DateType">
						<xsd:annotation>
							<xsd:documentation>On a NewRx the WrittenDate indicates the date the prescriber created this prescription being transmitted. It is recommended that transmission of the NewRx should be within 72 hours of the WrittenDate, with exceptions for state/federal regulations timeframe requirements. WrittenDate must precede or be equal to the transmission date. For future dating, see EffectiveDate.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LastFillDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExpirationDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EffectiveDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>EffectiveDate: The date or date/time after which this prescription being transmitted can be dispensed (i.e. do not fill before date) as authorized by the prescriber. For receipt of prescriptions with transmission of the NewRx greater than 72 hours of the WrittenDate, the RxChange transaction can be used for clarification with the prescriber. EXCEPTION: Electronic prescriptions for patients receiving Long Term Care Pharmacy Services are exempt from the EffectiveDate usage stated above.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PeriodEnd" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DeliveredOnDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DateValidated" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Substitutions" type="ecl:SubstitutionCodeRestricted">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="NumberOfRefills" type="datatypes:n1..2">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Diagnosis" type="structures:Diagnosis" minOccurs="0" maxOccurs="2">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CouponNumber" type="datatypes:an1..35" minOccurs="0"/>
					<xsd:element name="PriorAuthorization" type="datatypes:an1..35" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Note" type="datatypes:an1..210" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation" minOccurs="0" maxOccurs="5">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode" minOccurs="0" maxOccurs="5">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OtherCompoundInformation" type="structures:OtherCompoundInformationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Sig" type="structures:Sig">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Compound" type="datatypes:CompoundIngredientLotNotUsed" minOccurs="0" maxOccurs="25">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SoldDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PharmacyRequestedRefills" type="datatypes:n1..2" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DoNotFill" type="ecl:DoNotFillCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="NeededNoLaterThanDate" type="datatypes:UtcDateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="TimeZone" type="datatypes:TimeZone" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="NeededNoLaterThanReason" type="datatypes:an1..70" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RefillsRemaining" type="datatypes:n1..2" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="HistoryPrescriberOrderNumber" type="datatypes:an1..35" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacy" type="structures:Pharmacy" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Prescriber" type="structures:PrescriberGeneralChoice" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="HistorySource" type="datatypes:HistorySourceWithoutPayment" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SplitScript" type="ecl:SplitScriptIndicator" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ManufacturerName" type="datatypes:an1..105" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Name of the manufacturer.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LotNumber" type="datatypes:LotNumber" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="RxFillIndicator" type="ecl:RxFillIndicatorType" minOccurs="0"/>
					<xsd:element name="OfficeOfPharmacyAffairsID" type="datatypes:an1..15" minOccurs="0" maxOccurs="10">
						<xsd:annotation>
							<xsd:documentation>Health Resources and Services Administration (HRSA) Office of Pharmacy Affairs (OPA) Identification Code associated with the eligibility of this prescription for drugs purchased pursuant to rights under Section 340B of the Public Health Service Act of 1992.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DeliveryRequest" type="ecl:DeliveryRequestCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicator of whether patient requests delivery of prescription.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DeliveryLocation" type="ecl:DeliveryLocationtCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>If patient specifies a delivery, this is the location for the delivery.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DiabeticSupply" type="datatypes:DiabeticSupplyType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>When sig is sent in text only, the route of administration is strongly recommended. When structured Sig is sent, the route of administration is required.  When sig is sent in text only, the frequency is strongly recommended. When structured Sig is sent, frequency is strongly recommended. When the structured Sig is used the TestingFrequency must match to the structured Sig FrequencyNumericValue. When ordering items such as test strips or lancets, the Quantity must reflect the package quantity of the item (i.e. 100 instead of 1 box).</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="InjuryRelated" type="ecl:InjuryRelatedCode" minOccurs="0"/>
					<xsd:element name="Service" type="structures:ServiceStructure" minOccurs="0"/>
					<xsd:element name="IVAdministration" type="structures:IVAdministrationType" minOccurs="0"/>
					<xsd:element name="SelfAdministrationAllowed" type="ecl:BooleanCode" minOccurs="0"/>
					<xsd:element name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AnticipatedDischargeDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CurrentTreatmentCycle" type="datatypes:n1..2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="NumberOfCyclesPlanned" type="datatypes:n1..2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Wound" type="structures:WoundStructure" minOccurs="0" maxOccurs="9"/>
					<xsd:element name="PrescriberCheckedREMS" type="ecl:PrescriberCheckedREMSCode">
						<xsd:annotation>
							<xsd:documentation>Identifies if the prescribing system has performed an inquiry to the REMS Administrator in order to verify the REMS component of the prescription.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="REMSPatientRiskCategory" type="datatypes:an1..35" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the risk of adverse event based on variables of the patient.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="REMSAuthorizationNumber" type="datatypes:an1..140" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Titration" type="structures:TitrationType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  NewRxRequestMedication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="NewRxRequestMedication">
		<xsd:annotation>
			<xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DrugDescription" type="datatypes:an1..105">
				<xsd:annotation>
					<xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugCoded" type="datatypes:DrugCoded" minOccurs="0"/>
			<xsd:element name="Quantity" type="datatypes:QuantityForPrescribed" minOccurs="0"/>
			<xsd:element name="DaysSupply" type="datatypes:n1..3" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WrittenDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value 85.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastFillDate" type="datatypes:DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Last fill date may contain the last known fill date if the requesting pharmacy is leveraging information from the prescription label that was presented by the patient/caregiver.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpirationDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value 36.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EffectiveDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value 07.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PeriodEnd" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value  PE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeliveredOnDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value BE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateValidated" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value BE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Substitutions" type="ecl:SubstitutionCodeRestricted" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="datatypes:n1..2" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="structures:DiagnosisRestricted" minOccurs="0" maxOccurs="2"/>
			<xsd:element name="CouponNumber" type="datatypes:an1..35" minOccurs="0"/>
			<xsd:element name="PriorAuthorization" type="datatypes:an1..35" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="datatypes:an1..210" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherCompoundInformation" type="structures:OtherCompoundInformationType" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="Sig" type="structures:Sig" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Compound" type="datatypes:CompoundIngredientLotNotUsed" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SoldDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PharmacyRequestedRefills" type="datatypes:n1..2" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DoNotFill" type="ecl:DoNotFillCode" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeededNoLaterThanDate" type="datatypes:UtcDateType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimeZone" type="datatypes:TimeZone" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeededNoLaterThanReason" type="datatypes:an1..70" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="RefillsRemaining" type="datatypes:n1..2" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HistoryPrescriberOrderNumber" type="datatypes:an1..35" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacy" type="structures:OptionalPharmacy" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="Prescriber" type="structures:HistoryPrescriberChoice" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="HistorySource" type="datatypes:HistorySourceWithoutPayment" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SplitScript" type="ecl:SplitScriptIndicator" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManufacturerName" type="datatypes:an1..105" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name of the manufacturer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotNumber" type="datatypes:LotNumber" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="RxFillIndicator" type="ecl:RxFillIndicatorType" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="Service" type="structures:ServiceStructure" minOccurs="0"/>
			<xsd:element name="IVAdministration" type="structures:IVAdministrationType" minOccurs="0"/>
			<xsd:element name="SelfAdministrationAllowed" type="ecl:BooleanCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AnticipatedDischargeDate" type="datatypes:DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CurrentTreatmentCycle" type="datatypes:n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfCyclesPlanned" type="datatypes:n1..2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
