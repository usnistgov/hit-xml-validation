			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PrescriberGeneralChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PrescriberGeneralChoice">
		<xsd:complexContent>
			<xsd:restriction base="structures:HistoryPrescriberChoice">
				<xsd:choice>
					<xsd:element name="NonVeterinarian" type="structures:PrescriberGeneral"/>
					<xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorPrescriberGeneral"/>
				</xsd:choice>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PriorPeriodCorrectionType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PriorPeriodCorrectionType">
		<xsd:sequence>
			<xsd:element name="CorrectionDate" type="datatypes:StartEndDateType" minOccurs="0"/>
			<xsd:element name="CorrectionType" type="datatypes:an" minOccurs="0"/>
			<xsd:element name="BenefitsCoordination" type="structures:BenefitsCoordinationForCensus" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Provider  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Provider">
		<xsd:annotation>
			<xsd:documentation>Used when medical provider is needed, not the prescriber. Designated Provider - The patient’s provider for which this service applies, if applicable. The designated provider may be the primary care provider, the medical home provider, the facility provider or other provider. This information is provided as reference.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="datatypes:NonVeterinarianID" minOccurs="0"/>
			<xsd:element name="Specialty" type="datatypes:an1..10" minOccurs="0"/>
			<xsd:element name="PracticeLocation" type="structures:PracticeLocationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="datatypes:NameType"/>
			<xsd:element name="FormerName" type="datatypes:NameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="datatypes:AddressType" minOccurs="0"/>
			<xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionDetailCommonType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionDetailCommonType">
		<xsd:sequence>
			<xsd:element name="QuestionID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the question within the question set. Must be unique per question in the transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SequenceNumber" type="datatypes:n1..3">
				<xsd:annotation>
					<xsd:documentation>Sequence number assigned by the payer to indicate the order of the question in relation to other questions in the question set. Must be unique per question in the transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionLevel" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Question level to indicate grouping of questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionText" type="datatypes:an1..2000">
				<xsd:annotation>
					<xsd:documentation>Question text to display to the provider.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReference" type="structures:CodedReference" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="DefaultNextQuestionID" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is not dependent on the answer to the question. Required when the QuestionType is date or free text or when the QuestionType is select or numeric and the NextQuestionID for the answer choices or numeric conditions are not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionDetailForRequestType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionDetailForRequestType">
		<xsd:complexContent>
			<xsd:extension base="structures:QuestionDetailCommonType">
				<xsd:sequence>
					<xsd:element name="QuestionType" type="structures:QuestionTypeForRequest"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionDetailForResponseType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionDetailForResponseType">
		<xsd:complexContent>
			<xsd:extension base="structures:QuestionDetailCommonType">
				<xsd:sequence>
					<xsd:element name="QuestionType" type="structures:QuestionType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetGroup  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetGroup">
		<xsd:sequence>
			<xsd:element name="Header" type="structures:QuestionSetHeader"/>
			<xsd:element name="Question" type="structures:QuestionDetailForResponseType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetGroupForRequest  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetGroupForRequest">
		<xsd:sequence>
			<xsd:element name="Header" type="structures:QuestionSetHeaderWithComment"/>
			<xsd:element name="Question" type="structures:QuestionDetailForRequestType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetGroupForResponse  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetGroupForResponse">
		<xsd:sequence>
			<xsd:element name="Header" type="structures:QuestionSetHeader"/>
			<xsd:element name="Question" type="structures:QuestionDetailForResponseType" maxOccurs="unbounded"/>
			<xsd:element name="DeadlineForReply" type="datatypes:DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetHeader  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetHeader">
		<xsd:sequence>
			<xsd:element name="QuestionSetID" type="datatypes:an1..70">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the question set. It should not be assumed this is a unique identifier over time or across payers.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetTitle" type="datatypes:an1..70">
				<xsd:annotation>
					<xsd:documentation>Title of the question set.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetDescription" type="datatypes:an1..2000">
				<xsd:annotation>
					<xsd:documentation>Descriptive information about the question set.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetContactCommunicationNumber" type="datatypes:CommunicationNumbersTypeRestricted"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetHeaderWithComment  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetHeaderWithComment">
		<xsd:complexContent>
			<xsd:extension base="structures:QuestionSetHeader">
				<xsd:sequence>
					<xsd:element name="QuestionSetComment" type="datatypes:an1..2000" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The vendor should always allow the provider to send comments in addition to answering the questions.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionType">
		<xsd:choice>
			<xsd:element name="Select" type="structures:PASelectType">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" type="datatypes:DateTypeFlag">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Numeric" type="structures:NumericTypeBasic">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FreeText" type="structures:FreeTextTypeBasic">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionTypeForRequest  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionTypeForRequest">
		<xsd:choice>
			<xsd:element name="Select" type="structures:PASelectTypeForRequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" type="structures:DateTypeForRequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Numeric" type="structures:NumericChoiceForPARequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FreeText" type="structures:AnswerForTextType">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSApprovedDetail  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSApprovedDetail">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="datatypes:an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationPeriod" type="structures:DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="datatypes:an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSDeniedType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSDeniedType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSNote" type="datatypes:an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSModelType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSModelType">
		<xsd:sequence>
			<xsd:element name="SolicitedModel" type="structures:REMSSolicitedType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSOpenType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSOpenType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeadlineForReply" type="datatypes:DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSet" type="structures:QuestionSetGroup" minOccurs="0"/>
			<xsd:element name="REMSNote" type="datatypes:an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSOpenTypeResponse  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSOpenTypeResponse">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MoreInformationRequired" type="structures:QuestionSetGroupForResponse"/>
			<xsd:element name="REMSNote" type="datatypes:an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSReasonClosedCodeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSReasonClosedCodeType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClosedReasonCode" type="ecl:ReasonCode20" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>Code indicating the reason for the prior authorization closure.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="datatypes:an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationDetail" type="structures:PAAuthorizationDetails" minOccurs="0" maxOccurs="5"/>
			<xsd:element name="AuthorizationPeriod" type="structures:DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="datatypes:an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSReasonCodeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSReasonCodeType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonCode" type="ecl:ReasonCode20" maxOccurs="10"/>
			<xsd:element name="REMSAuthorizationNumber" type="datatypes:an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationDetail" type="structures:PAAuthorizationDetails" minOccurs="0" maxOccurs="5"/>
			<xsd:element name="AuthorizationPeriod" type="structures:DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="datatypes:an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSResponseChoiceType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSResponseChoiceType">
		<xsd:choice>
			<xsd:element name="Approved" type="structures:REMSApprovedDetail">
				<xsd:annotation>
					<xsd:documentation>Either AuthorizationDetail or AuthorizationPeriod must be sent.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Denied" type="structures:REMSDeniedType"/>
			<xsd:element name="Open" type="structures:REMSOpenTypeResponse"/>
			<xsd:element name="Closed" type="structures:REMSReasonClosedCodeType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSResponseChoiceTypeRestricted  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSResponseChoiceTypeRestricted">
		<xsd:choice>
			<xsd:element name="Open" type="structures:REMSOpenType"/>
			<xsd:element name="Closed" type="structures:REMSReasonCodeType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSResponseStructureCommon  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSResponseStructureCommon">
		<xsd:sequence>
			<xsd:element name="ReferenceNumber" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResponseStatus" type="structures:REMSResponseChoiceType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSResponseStructureRestricted  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSResponseStructureRestricted">
		<xsd:sequence>
			<xsd:element name="ReferenceNumber" type="datatypes:an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResponseStatus" type="structures:REMSResponseChoiceTypeRestricted"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSSolicitedType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSSolicitedType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="datatypes:an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeadlineForReply" type="datatypes:DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSet" type="structures:QuestionSetGroupForRequest" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  RateOfAdministration  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="RateOfAdministration">
		<xsd:sequence>
			<xsd:element name="RateOfAdministration" type="datatypes:an1..11"/>
			<xsd:element name="RateUnitOfMeasure" type="datatypes:SigCode">
				<xsd:annotation>
					<xsd:documentation>Examples such as seconds, minutes, hours, days.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimePeriodBasis" type="datatypes:SigCode">
				<xsd:annotation>
					<xsd:documentation>Examples such as minute, hour, day. For example, a dosing basis unit of measure of mg/kg to be calculated for a 24 hour time period would have a final representation of mg/kg/day.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  RecertificationMedication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="RecertificationMedication">
		<xsd:complexContent>
			<xsd:restriction base="structures:MedicationAbstract">
				<xsd:sequence>
					<xsd:element name="DrugDescription" type="datatypes:an1..105">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugCoded" type="datatypes:DrugCoded" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Quantity" type="datatypes:QuantityForPrescribed"/>
					<xsd:element name="DaysSupply" type="datatypes:n1..3" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="WrittenDate" type="datatypes:DateType">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LastFillDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExpirationDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EffectiveDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PeriodEnd" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DeliveredOnDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DateValidated" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Substitutions" type="ecl:SubstitutionCodeRestricted">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="NumberOfRefills" type="datatypes:n1..2" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
