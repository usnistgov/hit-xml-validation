							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RefillsRemaining" type="datatypes:n1..2" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="HistoryPrescriberOrderNumber" type="datatypes:an1..35" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacy" type="structures:Pharmacy" minOccurs="0" maxOccurs="0">
