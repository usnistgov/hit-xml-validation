      <xsd:element minOccurs="0" name="ShippingInstructions" type="specialized:CFShipping"/>
      <xsd:element minOccurs="0" name="ShipToEntity" type="specialized:CFShipToEntity"/>
      <xsd:element name="Medication" type="structures:CFMedicationLotNotUsedInCompoundIngredient"/>
      <xsd:element name="CFRxLabel" type="specialized:CFRxOrderType"/>
      <xsd:element minOccurs="0" name="BenefitsCoordination" type="structures:BaseBenefitsCoordination">
        <xsd:annotation>
          <xsd:documentation>Indicates coverage’s known to the sending system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  CFRxOrderType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="CFRxOrderType">
    <xsd:sequence>
      <xsd:element name="FillNumber" type="datatypes:n2">
        <xsd:annotation>
          <xsd:documentation>Defines the dispensing episode as an initial fill or an authorized refill. Values: 00 = initial fill, 01 = first refill, 02 = second refill, etc. Allowed values 00 thru 99. This element must always be two significant digits (e.g. 01, 02, 08, 14, 99).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RxBarCode" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Barcode representation of prescription as designated by pharmacy.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ChildResistantPackage" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>Indicator the prescription requires child resistant packaging.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CFGenericSubstitutedForBrand" type="specialized:CFGenericSubstitutedForBrand"/>
      <xsd:element minOccurs="0" name="Technician" type="specialized:CFRxEntryTechnician">
        <xsd:annotation>
          <xsd:documentation>Designated prescription entry tech for transaction</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RxRefillMessage" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Number of refills remaining and expiration date of prescription.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CFDrugImprint" type="specialized:CFDrugImprint">
        <xsd:annotation>
          <xsd:documentation>Drug Imprint information for the medication requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CFMonograph" type="specialized:CFMonograph">
        <xsd:annotation>
          <xsd:documentation>Drug Monograph information for the medication requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="10" minOccurs="0" name="CFWarningLabel" type="structures:CFWarningLabel">
        <xsd:annotation>
          <xsd:documentation>Warning label information for the medication requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Counsel" type="specialized:CFCounsel">
        <xsd:annotation>
          <xsd:documentation>Counseling Required indicator and text.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IngredientCost" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Ingredient cost of the medication dispensed.  Include decimal and sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PrescriptionSellPrice" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Total amount paid by all plans involved or cash prescription selling price. Include decimal and sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TotalAmountBrandPenalty" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Total amount of penalty by all plans involved for the selection of the brand name drug.  Include decimal and sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PatientPayAmount" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Field 505-F5 - Amount that is calculated by the processor and returned to the pharmacy as the TOTAL amount to be paid by the patient to the pharmacy; the patient’s total cost share, including copayments, amounts applied to deductible, over maximum amounts, penalties, etc. This is the patient pay amount from the final payer. Include decimal and sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CalculatedPrescriptionSellingPrice" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Calculated selling price for the prescription.  Could be U and C or Gross Amount Due.  Include decimal and sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DiscountAmount" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Amount of discount that was applied to the prescription.  Include decimal and sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ShippingAmount" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Amount of shipping cost that was applied to the prescription.  Include decimal and sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AuthorizationNumber" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Field 503-F3 - Number assigned by the processor to identify an authorized 		transaction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
