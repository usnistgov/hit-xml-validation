  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  TransferType  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="TransferType">
    <xsd:annotation>
      <xsd:documentation>Indicates file content.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="TRANSFER TO PHARMACY">
        <xsd:annotation>
          <xsd:documentation>Not Specified</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="TRANSFER FROM PHARMACY">
        <xsd:annotation>
          <xsd:documentation>Open Refill (Refills remaining) - This indicates that the transfer file only contains prescriptions with remaining open refills.  There will be one record for open refill.  For example:  If a prescription has been filled 3 times and has 2 more refills remaining, the file will contain one record indicating that 2 more refills are outstanding.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  TreatmentIndicatorCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="TreatmentIndicatorCode">
    <xsd:annotation>
      <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="NEW">
        <xsd:annotation>
          <xsd:documentation>Indicates the therapy is new to the patient.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CONTINUATION">
        <xsd:annotation>
          <xsd:documentation>Indicates a continuation of an existing therapy.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="RESTART">
        <xsd:annotation>
          <xsd:documentation>Indicates a prior therapy has been restarted.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
