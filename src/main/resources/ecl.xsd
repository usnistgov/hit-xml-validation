        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AB">
        <xsd:annotation>
          <xsd:documentation>Patient never under  Provider care</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="AC">
        <xsd:annotation>
          <xsd:documentation>Patient no longer under  Provider care</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
