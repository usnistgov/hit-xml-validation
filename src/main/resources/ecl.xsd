          <xsd:documentation>First DataBank Ingredient List ID (HICL_SEQ_NO)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="16">
        <xsd:annotation>
          <xsd:documentation>Micromedex/Medical Economics Generic Formulation Code (GFC)</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="32">
