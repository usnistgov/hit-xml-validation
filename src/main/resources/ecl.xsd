  <!-- ================================================== -->
  <!-- =====  Simple Type Definitions  -->
  <!-- ================================================== -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  AdditionalFreeTextIndicatorTypeCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="AdditionalFreeTextIndicatorTypeCode">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="M">
        <xsd:annotation>
          <xsd:documentation>Mandatory, provider must supply additional free text</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="O">
        <xsd:annotation>
          <xsd:documentation>Optional, provider may supply additional free text</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="NA">
        <xsd:annotation>
          <xsd:documentation>Not Allowed, provider may not supply additional free text</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  AddressTypeQualifier  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="AddressTypeQualifier">
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="P">
        <xsd:annotation>
          <xsd:documentation>Pharmacy</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="C">
        <xsd:annotation>
          <xsd:documentation>Clinic</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="M">
        <xsd:annotation>
          <xsd:documentation>Mailbox</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="D">
        <xsd:annotation>
          <xsd:documentation>Prescriber</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="CF">
        <xsd:annotation>
          <xsd:documentation>Central Fill Facility</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ZZZ">
        <xsd:annotation>
          <xsd:documentation>Mutually Defined</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="PY">
        <xsd:annotation>
          <xsd:documentation>Payer</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="DIRECT">
        <xsd:annotation>
          <xsd:documentation>Direct Participant ID</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="REM">
        <xsd:annotation>
          <xsd:documentation>REMS Administrator.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  AdministrationIndicatorCodes  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="AdministrationIndicatorCodes">
    <xsd:annotation>
      <xsd:documentation>Used for Sig</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="MANUFACTURERSINSTRUCTIONS">
        <xsd:annotation>
          <xsd:documentation>Manufacturer's Instructions</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  BooleanCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="BooleanCode">
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="Y">
        <xsd:annotation>
          <xsd:documentation>Yes</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="N">
        <xsd:annotation>
          <xsd:documentation>No</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ChangeOfPrescriptionStatusCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="ChangeOfPrescriptionStatusCode">
    <xsd:annotation>
      <xsd:documentation>Used in the Cancel Request message when the prescriber wishes to notify the pharmacy to no longer continue dispensing any open refills on an active prescription or to cancel a prescription that has not yet been dispensed.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="C">
        <xsd:annotation>
          <xsd:documentation>Cancel - Prescriber wishes to notify the pharmacy to cancel a prescription that has not yet been dispensed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="D">
        <xsd:annotation>
          <xsd:documentation>Discontinue - Prescriber wishes to notify the pharmacy to no longer continue dispensing any open refills on an active prescription.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ClinicalInfoRequestType  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="ClinicalInfoRequestType">
    <xsd:annotation>
      <xsd:documentation>Used in the Clinical Info Request message to indicate what type of patient centric clinical information is being requested.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="All">
        <xsd:annotation>
          <xsd:documentation>Request for all clinical information available to be shared for a Patient.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Allergies">
        <xsd:annotation>
          <xsd:documentation>Request for a Patient's Allergies</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Conditions">
        <xsd:annotation>
          <xsd:documentation>Request for a Patient's Conditions.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Medical History">
        <xsd:annotation>
          <xsd:documentation>Request for a Patient's Medical History.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Code  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="Code">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([!-~]|[ ])*[!-~]([!-~]|[ ])*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ComparisonOperatorCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="ComparisonOperatorCode">
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="EQ"/>
      <xsd:enumeration value="GT"/>
      <xsd:enumeration value="LT"/>
      <xsd:enumeration value="GE"/>
      <xsd:enumeration value="LE"/>
      <xsd:enumeration value="NE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ComparisonOperatorCodeRestricted  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="ComparisonOperatorCodeRestricted">
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="GT"/>
      <xsd:enumeration value="LT"/>
      <xsd:enumeration value="GE"/>
      <xsd:enumeration value="LE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  CompoundCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="CompoundCode">
    <xsd:annotation>
      <xsd:documentation>Code indicating whether or not the prescription is a compound.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="0">
        <xsd:annotation>
          <xsd:documentation>Not Specified This value is not allowed for the Telecommunication Standard</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation>Not a Compound—Medication that is available commercially as a dispensable product</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation>Compound – Customized medication prepared in a pharmacy by combining, mixing, or altering of ingredients (but not reconstituting) for an individual patient in response to a licensed practitioner’s prescription</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ConsentCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="ConsentCode">
    <xsd:annotation>
      <xsd:documentation>Patient Consent Indicator</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="Y">
        <xsd:annotation>
          <xsd:documentation>Patient gave consent for the information to be disclosed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="N">
        <xsd:annotation>
          <xsd:documentation>Patient consent not given.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="P">
        <xsd:annotation>
          <xsd:documentation>Patient gave consent for prescriber to only receive the medication history this prescriber prescribed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="X">
        <xsd:annotation>
          <xsd:documentation>Parental/Guardian gave consent for the information to be disclosed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Z">
        <xsd:annotation>
          <xsd:documentation>Parental/Guardian consent on behalf of a minor for prescriber to only receive the medication history this prescriber prescribed.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DUEClinicalSignificanceCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:simpleType name="DUEClinicalSignificanceCode">
    <xsd:annotation>
      <xsd:documentation>Code identifying the significance or severity level of a clinical event as contained in the originating database.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="ecl:Code">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation>Major - Code indicating that an event, transaction, etc. is of the highest importance; action required to prevent adverse drug event.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation>Moderate – Code indicating that an event, transaction, etc. is of mid-level significance; requires thoughtful review before prescribing/dispensing the medication. Risk vs. benefit should be evaluated. </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation>Minor – Code indicating a non-life threatening, annoying, or now-well-documented effect which may or may not require a change in drug therapy. </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="9">
        <xsd:annotation>
          <xsd:documentation>Undetermined - value to describe a professional service with variable or unknown severity.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DUECoAgentIDQualifierCode  <<ncpdpecl>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
