      <xsd:element name="Start" type="datatypes:DateType"/>
      <xsd:element minOccurs="0" name="End" type="datatypes:DateType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
