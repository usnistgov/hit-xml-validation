  <!--  SNOMEDCodeTextMandatory  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SNOMEDCodeTextMandatory">
    <xsd:sequence>
      <xsd:element name="Text" type="datatypes:an"/>
      <xsd:element name="Code" type="ecl:Code"/>
    </xsd:sequence>
  </xsd:complexType>
