          <xsd:element minOccurs="0" name="DEANumber" type="datatypes:an1..35"/>
          <xsd:element minOccurs="0" name="HIN" type="datatypes:an1..35"/>
          <xsd:element minOccurs="0" name="NAICCode" type="datatypes:an1..35"/>
          <xsd:element minOccurs="0" name="SocialSecurity" type="datatypes:an1..35"/>
