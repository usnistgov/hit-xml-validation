      <xsd:element minOccurs="0" name="MedicaidNumber" type="datatypes:an1..35"/>
      <xsd:element minOccurs="0" name="UPIN" type="datatypes:an1..35"/>
      <xsd:element minOccurs="0" name="FacilityID" type="datatypes:an1..35"/>
      <xsd:element minOccurs="0" name="PayerID" type="datatypes:an1..80"/>
      <xsd:element minOccurs="0" name="ProcessorIdentificationNumber" type="datatypes:an1..35"/>
      <xsd:element minOccurs="0" name="BINLocationNumber" type="datatypes:an1..35"/>
      <xsd:element minOccurs="0" name="MedicalRecordIdentificationNumberEHR" type="datatypes:an1..35"/>
